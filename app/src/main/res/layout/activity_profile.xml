<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingRight="3dp"
    android:paddingLeft="3dp"
    android:paddingTop="5dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.skvisli.treningsapp.Profile"
    android:background="#ffffff">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout5" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/hjem"
            android:src="@drawable/hjemikon"
            android:background="#ffffff"
            android:padding="1dp"
            android:onClick="goTo" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/konkurranse"
            android:src="@drawable/konkurranseikon"
            android:background="#ffffff"
            android:onClick="goTo" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/skritt"
            android:src="@drawable/skrittikon"
            android:background="#ffffff"
            android:onClick="goTo" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/resultat"
            android:src="@drawable/pallikon"
            android:background="#ffffff"
            android:onClick="goTo" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/profil"
            android:src="@drawable/profilikon"
            android:background="#ffffff" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/instilling"
            android:src="@drawable/instillingikon"
            android:background="#ffffff"
            android:onClick="goTo" />
    </LinearLayout>

    <ImageView
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:src="@drawable/strek"
        android:background="#000000"
        android:layout_below="@+id/linearLayout5"
        android:layout_alignEnd="@+id/linearLayout5" />

    <ImageView
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:src="@drawable/strek"
        android:background="#000000"
        android:layout_below="@+id/linearLayout5"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Sondre Kvisli"
        android:id="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="10 000 steps"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="1. place"
        android:id="@+id/textView"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="46dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Placements on podium"
        android:id="@+id/textView4"
        android:layout_marginTop="57dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Total number of steps"
        android:id="@+id/textView5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="58dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="500 000"
        android:id="@+id/textView6"
        android:layout_alignTop="@+id/textView5"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/medalje"
        android:layout_alignTop="@+id/textView"
        android:layout_toEndOf="@+id/textView5"
        android:layout_alignStart="@+id/imageView3"
        android:layout_above="@+id/textView4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/pall"
        android:layout_alignTop="@+id/textView4"
        android:layout_toEndOf="@+id/textView2"
        android:layout_alignBottom="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x6"
        android:id="@+id/textView9"
        android:layout_alignTop="@+id/imageView3"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x3"
        android:id="@+id/textView8"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x10"
        android:id="@+id/textView7"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignTop="@+id/textView7"
        android:src="@drawable/trofe"
        android:layout_toEndOf="@+id/textView2"
        android:layout_above="@+id/textView" />
</RelativeLayout>
